{
  "name": "starter",
  "version": "1.0.0",
  "description": "Starter Template for react projects",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --port 8081 --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
