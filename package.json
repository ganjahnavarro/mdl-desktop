    "express": "^4.14.0",
    "js-base64": "^2.1.9",
    "react": "^15.4.2",
    "react-addons-update": "^15.5.2",
    "react-data-grid": "^2.0.27",
    "react-data-grid-addons": "^2.0.27",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.0",
    "react-select": "^1.0.0-rc.3",
    "store": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
